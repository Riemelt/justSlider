{
  "name": "just-slider",
  "version": "1.7.0",
  "description": "Slider plugin for jQuery",
  "main": "./dist/just-slider.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "set NODE_ENV=development && webpack serve --config ./webpack.demo.config.cjs",
    "dev": "set NODE_ENV=development && webpack --config ./webpack.main.config.cjs",
    "build": "set NODE_ENV=production && webpack --config ./webpack.main.config.cjs",
    "build:demo": "set NODE_ENV=production && webpack --config ./webpack.demo.config.cjs",
    "dev:demo": "set NODE_ENV=development && webpack --config ./webpack.demo.config.cjs",
    "deploy": "gh-pages -d demo",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./jest-config.ts"
    ],
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Riemelt/justSlider.git"
  },
  "keywords": [
    "slider",
    "plugin",
    "jQuery"
  ],
  "author": "Riemelt <n.lychenkov@gmail.com> (https://github.com/Riemelt)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Riemelt/justSlider/issues"
  },
  "homepage": "https://github.com/Riemelt/justSlider#readme",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/jquery": "^3.5.14",
    "@types/jsdom": "^20.0.1",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-jest": "^29.1.2",
    "eslint": "^8.24.0",
    "gh-pages": "^4.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.3.1",
    "jquery": "^3.6.1",
    "jsdom": "^20.0.2",
    "postcss": "^8.4.17",
    "pug-plugin": "^4.5.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.0-rc.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^4.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
